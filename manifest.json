{
	"content_scripts": [
		{
			"js": [
				"script.js"
			],
			"css": [
				"style.css"
			],
			"run_at": "document_end",
			"matches": [
				"*://torrentz.*/*",
			]
		}
	],
	"description": "Adds IMDb information to torrent files on Torrentz.",
	"manifest_version": 2,
	"name": "IMDb for Torrentz",
	"version": "0.2",
	"icons": {
		"16": "icon-16.png",
		"48": "icon-48.png",
		"128": "icon-128.png"
	}
}